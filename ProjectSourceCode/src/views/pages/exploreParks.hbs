<div class="container mt-5">
                    

        <div class="row"> 
            <div class="col-md-6">
                <h1 class="mb-4"> Explore Parks</h1>
            </div>
            <div class="col-md-6">
             <form method='POST' action="/exploreParks">
                <input type="keyword" name="keyword" placeholder="Search"/>
                 <button id="submit_button" class="btn btn-primary" type="submit">S</button>
                 
            </form> 
            </div>
        </div>
        <div class="row">
            {{#each data}}
            <div class="col-md-4 mb-4">
                <div class="card">
                    <img class="card-img-top" src="{{this.data.url}}" alt="Event Image">
                    <div class="card-body">
                        <h5 class="card-title">{{this.data.fullName}}</h5>
                        <p class="card-text">Location: {{this.data.State }}</p>
                        <p class="card-text">Park Code: {{this.data.park_code}}</p>
                        <a href="google.com" class="btn btn-primary">Create Trip</a>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.4/dist/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
